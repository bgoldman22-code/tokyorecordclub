{
  "name": "cathedral-fm",
  "version": "1.0.0",
  "type": "module",
  "description": "Music recommendation engine that analyzes your Spotify taste and generates themed weekly playlists",
  "scripts": {
    "dev": "netlify dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\" \"functions/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@netlify/blobs": "^7.2.0",
    "@netlify/functions": "^2.4.1",
    "cookie": "^0.6.0",
    "framer-motion": "^10.16.16",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.11.0",
    "openai": "^6.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.10.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@netlify/plugin-sitemap": "^0.8.1",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "lucide-react": "^0.303.0",
    "netlify-cli": "^17.10.1",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
